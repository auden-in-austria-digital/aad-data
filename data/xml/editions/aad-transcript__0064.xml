  <text>
    <body>
      <div type="transcription" xml:id="transcription_aad_0064" xml:lang="de">
        <div type="letter">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00249/" ed="1r" type="card"/>
          <div hand="#print_white_penrose" xml:lang="en">
            <ab>
              <lb/><rs type="org" ref="#amp_organization_6">CHRIST CHURCH</rs><unclear>,</unclear> <rs type="place" ref="#amp_place_id_46">OXFORD</rs>
            </ab>
          </div>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00250/" ed="1v" type="card"/>
          <div hand="#hand_blue_auden" xml:lang="en">
            <ab>
              <lb/><hi rend="underline" hand="#hand_blue_auden">Air-Ma<unclear>il</unclear><space quantity="1" unit="char"/></hi>
            </ab>
          </div>
          <div hand="#print_brown_penrose" xml:lang="en">
            <ab>
              <lb/>POST CARD
            </ab>
            <ab>
              <lb/>925
            </ab>
            <ab>
              <seg>
                <lb/><rs type="org" ref="#amp_organization_6">CHRIST CHURCH</rs> <rs type="place" ref="#amp_place_id_46">OXFORD</rs>
              </seg>
              <seg>
                <lb/>Published by <rs type="org" ref="#amp_organization_110">Penrose &#38; Palmer</rs>, <rs type="place" ref="#amp_place_id_46">Oxford</rs>, and
                <lb/>Printed by <rs type="org" ref="#amp_organization_111">The Vandyck Printers Ltd.</rs>, at <rs type="place" ref="#amp_place_id_376">Bristol</rs>
              </seg>
            </ab>
          </div>
          <div type="letter_message" hand="#hand_blue_auden">
            <p>
              <lb/><rs type="org" ref="#amp_organization_6">Hier</rs> bin ich
              <lb/>für z<subst><del rend="overwritten" hand="#hand_blue_auden">e</del><add place="superimposed" hand="#hand_blue_auden">w</add></subst>ei Tage<unclear>.</unclear>
            </p>
            <closer>
              <salute>
                <lb/>Alles Gutes
              </salute>
              <salute>
                <lb/>Dein
              </salute>
              <signed>
                <lb/><rs type="person" ref="#amp_person_1">Wystan</rs>
              </signed>
            </closer>
          </div>
          <div hand="#hand_blue_auden">
            <ab>
              <lb/><rs type="person" ref="#amp_person_107">Herrn
              <lb/>Hugo Kurka</rs>
              <address>
                <lb/><rs type="place" ref="#amp_place_id_356"><foreign xml:lang="en">Vienna</foreign> XX
                <lb/>Adalbert Stifte<unclear>r</unclear>gasse 22/28
                <lb/><foreign xml:lang="en">Austria</foreign></rs>
              </address>   
            </ab>
          </div>
        </div>
      </div>
    </body>
