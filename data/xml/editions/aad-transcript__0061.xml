  <text>
    <body>
      <div type="transcription" xml:id="transcription_aad_0061" xml:lang="de">
        <div type="prose">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00243/" ed="1r" type="card"/>
          <ab hand="#print_gray_sdr">
            <lb/><rs type="event" ref="#amp_event_id_118">SCHWETZINGER FESTSPIELE</rs> 1961
          </ab>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00244/" ed="1v" type="card"/>
          <ab hand="#print_gray_sdr">
            <lb/>Der <rs type="person" ref="#amp_person_262">Intendant des <rs type="org" ref="#amp_organization_103">Süddeutschen Rundfunks</rs></rs>
            <lb/>gibt sich die Ehre,
            <lb/>im Anschluß an die <rs type="event" ref="#amp_event_id_117">Vorstellung</rs> zu einem
            <lb/><hi rend="italic">EMPFANG</hi>
            <lb/>in das <rs type="place" ref="#amp_place_id_370">Hotel &#187;Europäischer Hof&#171;</rs> in <rs type="place" ref="#amp_place_id_369">Heidelberg</rs>
            <lb/>einzuladen.
          </ab>
          <ab hand="#print_gray_sdr">
            <seg>
              <lb/>Samstag, 20. Mai 1961
            </seg>
            <seg>
              <lb/><rs type="event" ref="#amp_event_id_117">Uraufführung</rs>
            </seg>
            <seg>
              <lb/><hi rend="italic">&#187;<rs type="lit_work" ref="#lit_work_id_211">ELEGIE FÜR JUNGE LIEBENDE</rs>&#171;</hi>
            </seg>
            <seg>
              <lb/>Oper in 3 Akten von <rs type="person" ref="#amp_person_1">W. H. Auden</rs> und <rs type="person" ref="#amp_person_4">Chester Kallman</rs>
            </seg>
            <seg>
              <lb/>Musik von <rs type="person" ref="#amp_person_79">Hans Werner Henze</rs>
            </seg>
          </ab>
        </div>
      </div>
    </body>
