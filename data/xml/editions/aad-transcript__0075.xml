  <text>
    <body>
      <div type="transcription" xml:id="transcription_aad_0075" xml:lang="de">
        <div type="letter">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00273/" ed="1r" type="card"/>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00274/" ed="1v" type="card"/>
          <div hand="#hand_blue_auden" xml:lang="it">
            <ab>
              <lb/>Via <hi rend="underline" hand="#hand_blue_auden">A</hi>erea
            </ab>
          </div>
          <div hand="#print_gray_alterocca" xml:lang="it">
            <ab>
              <seg>
                <lb/><rs type="place" ref="#amp_place_id_399">LACCO AMENO</rs>
              </seg>
              <seg>
                <seg>
                  <lb/>Panorama di notte
                </seg>
                <seg xml:lang="en">
                  <lb/>Panorama by night
                </seg>
                <seg xml:lang="fr">
                  <lb/>Panorama de nuit
                </seg>
                <seg xml:lang="de">
                  <lb/>Panorama bei Nacht
                </seg>
              </seg>
            </ab>
          </div>
          <div type="letter_message" hand="#hand_blue_auden">
            <p>
              <lb/>Bin in <rs type="place" ref="#amp_place_id_159">Italien</rs> für
              <lb/>eine Woche.
            </p>
            <closer>
              <salute>
                <lb/>Alles Gutes
              </salute>
              <signed>
                <lb/><rs type="person" ref="#amp_person_1">Wystan</rs>
              </signed>
            </closer>
          </div>
          <div type="letter_message" hand="#hand_blue_kallman">
            <p>
              <lb/>Ich auch&#x2013;und<hi rend="underline" hand="#hand_blue_kallman"> sehr</hi> <unclear>K</unclear>alt ist'<unclear><hi rend="underline" hand="#hand_blue_kallman"><subst><del rend="overwritten whiteout" hand="#hand_blue_kallman">e</del><add place="superimposed" hand="#hand_blue_kallman">s</add></subst></hi><del rend="whiteout" hand="#hand_blue_kallman">.</del></unclear>
              <lb/>für eine Griechische Wei<unclear>b.</unclear>
            </p>
            <closer>
              <signed>
                <lb/><rs type="person" ref="#amp_person_4">Chester</rs>.
              </signed>
            </closer>
          </div>
          <div type="letter_message" hand="#hand_blue_pittas">
            <p>
              <lb/>mit beste Grüsse
              <lb/>aus <rs type="place" ref="#amp_place_id_400">Forio</rs>,<rs type="person" ref="#amp_person_251" ana="#ana_085">Konstantin</rs>
            </p>
            <ab>
              <lb/><gap reason="illegible" unit="char" atLeast="1"/>
            </ab>
          </div>
          <div hand="#print_gray_alterocca" xml:lang="it">
            <ab>
              <lb/>F. 63984
            </ab>
            <ab>
              <lb/><rs type="org" ref="#amp_organization_142">ALTEROCCA</rs>
              <lb/><rs type="place" ref="#amp_place_id_397">TERNI</rs>
            </ab>
            <ab>
              <lb/>FOTOCOLOR - RIPRODUZIONE VIETATA
            </ab>
          </div>
          <div hand="#hand_blue_auden">
            <ab>
              <lb/><rs type="person" ref="#amp_person_107">Her<unclear>r</unclear>n
              <lb/>Hugo Kurka</rs>
              <address>
                <lb/><rs type="place" ref="#amp_place_id_356">Wien XX
                <lb/>Adalbert Stiftergasse
                <lb/>22/28
                <lb/>Austria</rs>
              </address>   
            </ab>
          </div>
        </div>
      </div>
    </body>
