  <text>
    <body>
      <div type="transcription" xml:id="transcription_aad_0068" xml:lang="de">
        <div type="letter">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00257/" ed="1r" type="card"/>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00258/" ed="1v" type="card"/>
          <div>
            <ab hand="#hand_blue_auden">
              <lb/><hi rend="underline" hand="#hand_blue_auden">Flug-<space quantity="1" unit="char"/>Post</hi>
            </ab>
          </div>
          <div>
            <ab hand="#print_black_mittet" xml:lang="en">
              <seg>
                <lb/><rs type="place" ref="#amp_place_id_384">Trondheim</rs>, <rs type="place" ref="#amp_place_id_383">Norway</rs>.
              </seg>
              <seg xml:lang="no">
                <lb/>Bryggene ved <rs type="place" ref="#amp_place_id_385">Nidelven</rs>.
              </seg>
              <seg>
                <lb/>Old warehouses by the <rs type="place" ref="#amp_place_id_385">River Nid</rs>.
              </seg>
            </ab>
          </div>
          <div type="letter_message" hand="#hand_blue_kallman">
            <opener>
              <dateline>
                <date>
                  <lb/>25<unclear>-</unclear>5<unclear>-</unclear>61
                </date>
              </dateline>
            </opener>
            <p>
              <lb/>Die Blumen sind fantastisch
              <lb/>anzusehen<unclear>.</unclear> Aber bis jetzt<unclear>&#717;&#717;&#717;&#717;&#717;&#717;</unclear>
            </p>
            <closer>
              <signed>
                <lb/><rs type="person" ref="#amp_person_4">Chester</rs>
              </signed>
            </closer>
          </div>
          <div type="letter_message" hand="#hand_blue_auden">
            <p>
              <lb/>Jezt geht es nie d<unclear>u</unclear>nkel
            </p>
            <closer>
              <salute>
                <lb/>Grüsse
              </salute>
              <signed>
                <lb/><rs type="person" ref="#amp_person_1">Wystan</rs>
              </signed>
            </closer>
          </div>
          <div hand="#print_black_mittet" xml:lang="en">
            <ab >
              <lb/>886/26
            </ab>
            <ab>
              <lb/>PRINTED IN <rs type="place" ref="#amp_place_id_383">NORWAY</rs>
            </ab>
          </div>
          <div>
            <ab hand="#hand_blue_auden">
              <lb/><rs type="person" ref="#amp_person_107">Hugo</rs> und <rs type="person" ref="#amp_person_240">Christa Kurka</rs>
              <address>
                <lb/><rs type="place" ref="#amp_place_id_371">Wien IX
                <lb/>Mühlnergasse 14/4
                <lb/><foreign xml:lang="en">Austria</foreign> <unclear>.</unclear></rs>
              </address>   
            </ab>
          </div>
        </div>
      </div>
    </body>
  </text>
