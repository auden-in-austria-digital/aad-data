  <text>
    <body>
      <div type="transcription" xml:id="transcription_aad_0066" xml:lang="de">
        <div type="letter">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00253/" ed="1r" type="card"/>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00254/" ed="1v" type="card"/>
          <div hand="#hand_blue_auden" xml:lang="en">
            <ab>
              <lb/>Ai<hi rend="underline" hand="#hand_blue_auden">r-Mai</hi>l
            </ab>
          </div>
          <div hand="#print_brown_crocker" xml:lang="en">
            <ab>
              <lb/>CGO-38
            </ab>
            <ab>
              <seg>
                <lb/><rs type="place" ref="#amp_place_id_379">MICHIGAN AVENUE</rs> SKYLINE
              </seg>
              <seg>
                <lb/><rs type="place" ref="#amp_place_id_129">Chicago</rs>'s famous "Lake Front" skyline as seen from
                <lb/>the <rs type="org" ref="#amp_organization_114">Museum of Natural History</rs>.
              </seg>
              <seg>
                <lb/>Color photo by <rs type="person" ref="#amp_person_266">Wm. H. Nawodylo</rs>, <rs type="place" ref="#amp_place_id_129">Chicago</rs>
              </seg>
            </ab>
          </div>
          <div type="letter_message" hand="#hand_blue_auden">
            <p>
              <lb/>Vie<unclear><subst><del rend="overwritten" hand="#hand_blue_auden">le</del><add place="superimposed" hand="#hand_blue_auden">l</add></subst></unclear> Dank für deinen
              <lb/>Bri<unclear>e</unclear>f. Bin <rs type="place" ref="#amp_place_id_129">hier</rs> für
              <lb/>zwei Tage.
            </p>
            <p>
              <lb/>Holfe<gap reason="illegible" unit="char" atLeast="1" atMost="2"/>lich hast Du
              <lb/>jezt einen Wohnung
              <lb/>Gefunden<unclear>.</unclear>
            </p>
            <closer>
              <salute>
                <lb/>grüsse
              </salute>
              <salute>
                <lb/>dein
              </salute>
              <signed>
                <lb/><rs type="person" ref="#amp_person_1">Wystan</rs>
              </signed>
            </closer>
          </div>
          <div hand="#print_brown_crocker" xml:lang="en">
            <ab>
              <lb/>PUBLISHED BY <rs type="org" ref="#amp_organization_113">H. S. CROCKER CO., INC.</rs>
              <lb/>350 NO. CLARK STREET, <rs type="place" ref="#amp_place_id_129">CHICAGO</rs> 10, ILLINOIS &#xB7; PRINTED IN <rs type="place" ref="#amp_place_id_15">U. S. A.</rs>
            </ab>
            <ab>
              <lb/>MIRRO-KROME CARD BY <rs type="org" ref="#amp_organization_113">H. S. CROCKER CO., INC.</rs>, <rs type="place" ref="#amp_place_id_161">SAN FRANCISCO 1, CALIF.</rs>
            </ab>
          </div>
          <div hand="#hand_blue_auden">
            <ab>
              <lb/><rs type="person" ref="#amp_person_107">Herrn
              <lb/>Hugo Kurka</rs>
              <address>
                <lb/><rs type="place" ref="#amp_place_id_371"><hi rend="underline" hand="#hand_blue_auden">WIEN IX</hi>
                <lb/>Mühlnergasse 14/4
                <lb/><foreign xml:lang="en">Austria</foreign></rs>
              </address>   
            </ab>
          </div>
        </div>
      </div>
    </body>
  </text>
