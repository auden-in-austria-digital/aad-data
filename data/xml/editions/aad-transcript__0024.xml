      <div type="transcription" xml:id="transcription_aad_0024" xml:lang="de">
        <div type="envelope">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00093/" ed="1r" type="envelope"/>
          <ab hand="#hand_blue_auden">
            <lb/>mit<hi rend="underline" hand="#hand_blue_auden"><space unit="char" quantity="1"/>Flug-Po</hi>st
          </ab>
          <ab hand="#hand_blue_auden">
            <lb/><rs type="person" ref="#amp_person_240">Frau Christa Kurka</rs>
            <address>
              <lb/><rs type="place" ref="#amp_place_id_356">Wien XX
              <lb/>Adalbert Stiftergasse 22/28</rs>
            </address>   
          </ab>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00094/" ed="1v" type="envelope"/>
          <ab hand="#print_black_auden">
            <lb/><rs type="person" ref="#amp_person_1">W. H. AUDEN</rs>
            <address>
              <lb/><rs type="place" ref="#amp_place_id_2"><del rend="overstrike" hand="#hand_blue_auden"><space unit="char" quantity="1"/>KIRCHSTETTEN (WESTBAHN</del>)
              <lb/><del rend="overstrike" hand="#hand_blue_auden">HINTERHOLZ 6</del>
              <lb/><del rend="overstrike" hand="#hand_blue_auden">N.-Ö., <foreign xml:lang="en">AUSTRIA</foreign></del></rs>
            </address>
            <seg hand="#hand_blue_auden">
              <address>
                <lb/><rs type="place" ref="#amp_place_id_19">Berlin-Dahlem
                <lb/>Hubertusbaderstrasse 43</rs>
              </address>
            </seg>
          </ab>
        </div>
        <div type="letter">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00095/" ed="2r" type="sheet"/>
          <div type="letter_message" hand="#hand_blue_auden">
            <opener>
              <dateline>
                <date>
                  <lb/>Nov 23<hi rend="superscript">rd</hi>.
                </date>
                <placeName>
                  <address>
                    <lb/><rs type="place" ref="#amp_place_id_19">Berli<unclear>n</unclear>-Dahle<unclear>m</unclear>
                    <lb/>Hubertusbaderstrasse 4<unclear>3</unclear>.</rs>
                  </address>
                </placeName>
              </dateline>
              <salute>
                <lb/>Lieber <rs type="person" ref="#amp_person_107">Hugo</rs> und <rs type="person" ref="#amp_person_240">Christa</rs>
              </salute>
            </opener>
            <p>
              <lb/>Hiermit 800.D.M. Ich glaube
              <lb/>es soll gut seine <subst><del rend="overwritten" hand="#hand_blue_auden">p</del><add place="superimposed" hand="#hand_blue_auden">P</add></subst>lätze <add place="above"  hand="#hand_blue_auden">(hin und zuruck)<space unit="char" quantity="1"/></add>am den Fl<unclear>eu</unclear>gzeug
              <lb/>so fort zu bestellen weil es wird
              <lb/>wahrscheinlich viel <subst><del rend="overwritten" hand="#hand_blue_auden">v</del><add place="superimposed" hand="#hand_blue_auden">V</add></subst>erkehr <del rend="overstrike" hand="#hand_blue_auden">w<gap reason="illegible" unit="char" atLeast="1" atMost="2"/></del> sein u<unclear>m</unclear>
              <lb/>Weinachtszeit<unclear>,</unclear>
            </p>
            <closer>
              <signed>
                <lb/><rs type="person" ref="#amp_person_1">Wystan</rs>
              </signed>
            </closer>
          </div>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00096/" ed="2v" type="sheet"/>
          <ab hand="#hand_blue_unident_0024">
            <lb/><space unit="char" quantity="1"/><hi rend="underline" hand="#hand_blue_unident_0024">384 &#x22C5; 6<unclear>.</unclear>5<unclear>,</unclear><space unit="char" quantity="1"/></hi>
            <lb/>2<unclear>3</unclear>04
            <lb/><space unit="char" quantity="1"/><hi rend="underline" hand="#hand_blue_unident_0024">192<subst><del rend="overwritten" hand="#hand_blue_unident_0024">2</del><add place="superimposed" hand="#hand_blue_unident_0024">0</add></subst><space unit="char" quantity="1"/></hi>
            <lb/><rs type="note" ana="#ana_060">2,49<unclear>6</unclear><del rend="overstrike" hand="#hand_blue_unident_0024">0</del></rs>
          </ab>
          <ab hand="#hand_blue_unident_0024">
            <lb/>768
          </ab>
          <ab hand="#hand_blue_unident_0024">
            <lb/>2,499<unclear>'</unclear>&#x2014;
            <lb/>4998
          </ab>
          <ab hand="#hand_blue_unident_0024">
            <lb/>5100<unclear>'</unclear>&#x2013;
          </ab>
          <ab hand="#hand_blue_unident_0024">
            <lb/>208<unclear>,</unclear>
            <lb/><hi rend="underline">102<unclear>,</unclear>&#x2014;</hi>
            <lb/><hi rend="double-underline">106&#x2014;</hi>
          </ab>
          <ab hand="#hand_blue_unident_0024">
            <lb/><space unit="char" quantity="1"/><hi rend="underline"><unclear>3</unclear>2&#x22C5;6,5<space unit="char" quantity="1"/></hi>
            <lb/>192
            <lb/><space unit="char" quantity="1"/><hi rend="underline">160<space unit="char" quantity="1"/></hi>
            <lb/><hi rend="double-underline">2080</hi>
          </ab>
        </div>
