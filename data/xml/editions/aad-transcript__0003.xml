  <text>
    <body>
      <div type="transcription" xml:id="transcription_aad_0003" xml:lang="de">
        <div type="envelope">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00009/" ed="1r" type="envelope"/>
          <ab hand="#hand_blue_auden">
            <lb/><rs type="person" ref="#amp_person_107">Herrn
            <lb/>Hug<unclear>o</unclear> Kurka</rs>
            <address>
              <lb/><rs type="place" ref="#amp_place_id_356">Wien XX
              <lb/>Adalbert Stiftergasse
              <lb/>22/28.</rs>
            </address>   
          </ab>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00010/" ed="1v" type="envelope"/>
        </div>
        <div type="letter">
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00011/" ed="2r" type="sheet"/>
          <div type="letter_message" hand="#hand_blue_auden">
            <opener>
              <dateline>
                <date>
                  <lb/>Montay
                </date>
              </dateline>
              <salute>
                <lb/>Liebe<unclear>r</unclear> <rs type="person" ref="#amp_person_107">Hugo</rs>:
              </salute>
            </opener>
            <p>
              <lb/>ich hoffe dass <rs type="person" ref="#amp_person_4">Chester</rs> hat nicht
              <lb/>vergessen mein Post-karte zu schicken.
            </p>
            <p>
              <lb/>Komme Mittwoch 19&#xB7;15.
            </p>
            <closer>
              <salute>
                <lb/>dein
              </salute>
              <signed>
                <lb/><rs type="person" ref="#amp_person_1">Wystan</rs>
              </signed>
            </closer>
          </div>
        <pb facs="https://iiif.acdh.oeaw.ac.at/aad/aad_00012/" ed="2v" type="sheet"/>
        </div>
      </div>
    </body>
  </text>
