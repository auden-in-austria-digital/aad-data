                <sch:rule context="//tei:body/tei:div/tei:div[@type='enclosure']">
                  <sch:assert test="*[self::tei:div[@type='poem' or @type='prose_translation' or @type='comments']]">type required or invalid type value; only poem, comments or prose_translation allowed</sch:assert>
                </sch:rule>
              </constraint>
            </constraintSpec>
            <attList>
