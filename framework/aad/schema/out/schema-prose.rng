               <value>other</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A user-defined rendition description language</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="tei_att.styleDef.attribute.schemeVersion">
