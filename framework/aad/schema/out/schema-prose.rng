            <oneOrMore>
               <ref name="tei_model.pLike"/>
            </oneOrMore>
            <group>
               <optional>
                  <ref name="tei_origin"/>
               </optional>
            </group>
         </choice>
         <ref name="tei_att.global.attributes"/>
         <empty/>
      </element>
